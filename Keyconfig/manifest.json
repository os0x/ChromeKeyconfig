{
  "name": "Keyconfig",
  "description": "Keyconfig for Google Chrome\u2122",
  "background": {
    "scripts": [
      "js/TabUtils.js",
      "js/QuickMarks.js",
      "js/background.js"
    ]
  },
  "permissions": [ "bookmarks", "tabs", "http://ss-o.net/" , "http://wedata.net/", "clipboardRead"],
  "options_page": "options_page.html",
  "version": "1.15.0",
  "default_locale": "en_US",
  "content_scripts": [
    {
      "js": [
        "js/chrome_keyconfig.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "128": "Keyconfig_128.png",
    "64": "Keyconfig_64.png",
    "32": "Keyconfig_32.png",
    "16": "Keyconfig.png"
  },
  "manifest_version": 2
}
